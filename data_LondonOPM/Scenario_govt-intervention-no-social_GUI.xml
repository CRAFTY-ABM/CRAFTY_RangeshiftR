<scenario startTick="1" endTick="10" world="GreaterLondon" 
version = "behaviour_baseline"
scenario = "govt-intervention-no-social">
  <capitals class="org.volante.abm.serialization.CSVCapitalLoader" file="csv/Capitals.csv" indexed="true"/>
  <services class="org.volante.abm.serialization.CSVServiceLoader" file="csv/Services.csv" indexed="true"/>
   <outputs outputDirectory="output/%v/%s" filePattern="%s-%i-%r-%o" tickPattern="%s-%i-%r-%o-%y">
     
      <output class="org.volante.abm.output.CellTable" perRegion="true" addRegion="false" addCellRegion="false" addServices="true" addCapitals="true" addCompetitiveness="true" addLandUseIndex="true" addLandUse="false" addAgent="true" addPreAllocCompetitiveness="false" addPreAllocLandUse="false" addGiThreshold="false" addPreAllocGuThreshold="false"  doubleFormat="0.000" startYear="1" everyNYears="1"/>
      <output class="org.volante.abm.output.AggregateAFTCompositionCSVOutputter" outputSums="true" perRegion="true" startYear="1" everyNYears="1"/>
      <output class="org.volante.abm.output.AggregateAFTCompetitivenessCSVOutputter" perRegion="true" startYear="1" everyNYears="1"/>
      <output class="org.volante.abm.output.AggregateDemandSupplyCSVOutputter" perRegion="true" startYear="1" everyNYears="1"/>
      <output class="org.volante.abm.output.TakeoverCellOutputter" perRegion="true" startYear="1" everyNYears="1" filePerTick="false" addRegion="false"/>
      <output class="org.volante.abm.output.GivingInStatisticsOutputter" startYear="1" perRegion="true" everyNYears="1" filePerTick="false" addRegion="false"/>
      <output class="org.volante.abm.output.ActionCSVOutputter" agentpattern=".*Inst" filePerTick="false" perRegion="true" startYear="1" everyNYears="1"/>
 
   </outputs>
  
 
   <displays class="org.volante.abm.visualisation.DefaultModelDisplays">
    <display class="org.volante.abm.visualisation.CapitalDisplay" initial="OPM_presence" title="Capitals"/>
    <display class="org.volante.abm.visualisation.ProductionDisplay" initial="biodiversity" title="Production"/>
    <!-- <display class="org.volante.abm.comi.visualisation.OPMAgents" title="AgentTypes"/>-->
    <display class="org.volante.abm.comi.visualisation.CSVAgentDisplayLoader" file="csv/AgentColors.csv" indexed="true" title="AgentTypes"/>
    <display class="org.volante.abm.visualisation.CompetitivenessDisplay" initial="no_mgmt" title="Competitiveness"/>
    <display class="org.volante.abm.visualisation.SubmodelDisplays" title="Submodels"/>
</displays>
 
   <worldLoaderFile>xml/World_withoutSN_multiplicativeNoise_linearcompetition_relative.xml</worldLoaderFile>
  </scenario>
