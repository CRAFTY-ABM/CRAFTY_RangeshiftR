<functionalRoles>
	<fRole class="org.volante.abm.comi.fr.ComiVariantProductionFR" label="mgmt_highInt" serialID="0"
		givingUpMean="${@(agents/AftParams_mgmt_highInt.csv, givingUpDistributionMean)}"
		givingInMean="${@(agents/AftParams_mgmt_highInt.csv, givingInDistributionMean)}"
		givingUpProbability = "${@(agents/AftParams_mgmt_highInt.csv, givingUpProb)}">
		<alternativeFr>mgmt_highInt</alternativeFr>

 		<givingUpDistribution class="com.moseph.modelutils.distribution.NormalDistribution" 
 			mean="${@(agents/AftParams_mgmt_highInt.csv, givingUpDistributionMean)}" 
 			sd="${@(agents/AftParams_mgmt_highInt.csv, givingUpDistributionSD)}"/>
 		
 		<givingInDistribution class="com.moseph.modelutils.distribution.NormalDistribution" 
 			mean="${@(agents/AftParams_mgmt_highInt.csv, givingInDistributionMean)}" 
 			sd="${@(agents/AftParams_mgmt_highInt.csv, givingInDistributionSD)}"/>
 		
 		<serviceLevelNoise class="com.moseph.modelutils.distribution.UniformDistribution" 
 			min="${@(agents/AftParams_mgmt_highInt.csv, serviceLevelNoiseMin)}" 
 			max="${@(agents/AftParams_mgmt_highInt.csv, serviceLevelNoiseMax)}"/>
 		
		<production class="org.volante.abm.example.SimpleProductionModel"
			csvFile="${@(agents/AftParams_mgmt_highInt.csv, productionCsvFile)}" multiplyProductionNoise="true" />
	</fRole>
	 

