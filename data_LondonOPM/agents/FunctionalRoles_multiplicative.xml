<functionalRoles>
	<fRole class="org.volante.abm.comi.fr.ComiVariantProductionFR" label="no_mgmt" serialID="0"
		givingUpMean="${@(agents/%v/AftParams_no_mgmt.csv, givingUpDistributionMean)}"
		givingInMean="${@(agents/%v/AftParams_no_mgmt.csv, givingInDistributionMean)}"
		givingUpProbability = "${@(agents/%v/AftParams_no_mgmt.csv, givingUpProb)}">
		<alternativeFr>no_mgmt</alternativeFr>

 		<givingUpDistribution class="com.moseph.modelutils.distribution.NormalDistribution" 
 			mean="${@(agents/%v/AftParams_no_mgmt.csv, givingUpDistributionMean)}" 
 			sd="${@(agents/%v/AftParams_no_mgmt.csv, givingUpDistributionSD)}"/>
 		
 		<givingInDistribution class="com.moseph.modelutils.distribution.NormalDistribution" 
 			mean="${@(agents/%v/AftParams_no_mgmt.csv, givingInDistributionMean)}" 
 			sd="${@(agents/%v/AftParams_no_mgmt.csv, givingInDistributionSD)}"/>
 		
 		<serviceLevelNoise class="com.moseph.modelutils.distribution.UniformDistribution" 
 			min="${@(agents/%v/AftParams_no_mgmt.csv, serviceLevelNoiseMin)}" 
 			max="${@(agents/%v/AftParams_no_mgmt.csv, serviceLevelNoiseMax)}"/>
 		
		<production class="org.volante.abm.example.SimpleProductionModel"
			csvFile="${@(agents/%v/AftParams_no_mgmt.csv, productionCsvFile)}" multiplyProductionNoise="true" />
	</fRole>
	 

